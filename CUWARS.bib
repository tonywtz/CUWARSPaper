}

@Article{Lustigetal2011,
  author     = {Hanno Lustig and Nikolai Roussanov and Adrien Verdelhan},
  journal    = {Review of Financial Studies},
  title      = {Common Risk Factors in Currency Markets},
  year       = {2011},
  number     = {11},
  pages      = {3731-3777},
  volume     = {24},
  abstract   = {We identify a \&quot;slope\&quot; factor in exchange rates. High interest rate currencies load more on this slope factor than low interest rate currencies. This factor accounts for most of the cross-sectional variation in average excess returns between high and low interest rate currencies. A standard, no-arbitrage model of interest rates with two factors--a country-specific factor and a global factor--can replicate these findings, provided there is sufficient heterogeneity in exposure to global or common innovations. We show that our slope factor identifies these common shocks, and we provide empirical evidence that it is related to changes in global equity market volatility. By investing in high interest rate currencies and borrowing in low interest rate currencies, U.S. investors load up on global risk. The Author 2011. Published by Oxford University Press on behalf of The Society for Financial Studies. All rights reserved. For Permissions, please e-mail: journals.permissions@oup.com., Oxford University Press.},
  bdsk-url-1 = {http://ideas.repec.org/a/oup/rfinst/v24yi11p3731-3777.html},
