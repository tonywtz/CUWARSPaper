@Article{EvansLewis1995,
  author     = {Evans, Martin D D and Lewis, Karen K},
  journal    = {Review of Financial Studies},
  title      = {Do Long-Term Swings in the Dollar Affect Estimates of the Risk Premia?},
  year       = {1995},
  number     = {3},
  pages      = {709-42},
  volume     = {8},
  abstract   = {Foreign exchange returns exhibit behavior difficult to reconcile with standard theoretical models. This article asks whether the recent findings of long swings in exchange rates between appreciating and depreciating periods affect estimates of the foreign exchange risk premium. We demonstrate how the \&quot;peso problem\&quot; introduced by expected shifts in exchange rate regimes can affect inferences about the risk premium in at least two ways: (1) it can make the foreign exchange risk premium appear to contain a permanent disturbance when it does not, and (2) it can induce bias in the foreign exchange return regressions such as in Fama (1984). Article published by Oxford University Press on behalf of the Society for Financial Studies in its journal, The Review of Financial Studies.},
